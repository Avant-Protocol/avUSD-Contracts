    "@nomicfoundation/ignition-core": "^0.15.6",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.9",
    "cross-env": "7.0.0",
    "dotenv": "^16.0.3",
